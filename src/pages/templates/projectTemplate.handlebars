<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="">
    <meta name="author" content="Bradley Anderson">

    <title>{{project}}</title>

    <!-- inject:css-->
    <link rel="stylesheet" href="/dist/css/style.min.css">
    <!-- endinject -->

    <link href="../resources/favicon.ico" rel="shortcut icon">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700,900" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic" rel="stylesheet" type="text/css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]--> 

</head>

<body id="page-top" class="index">

    {{> navbar }}

    <section id="project" class="first">
        <div class="container">
            <div class="row project-title">
                <div class="col-sm-12 col-md-8 col-md-offset-2 text-center">
                    <h2>{{project}}</h2>
                    <hr class="line-primary-2">
                </div>
            </div>

            <!-- Carousel================================================== -->
            <div class="row">
                <div id="myCarousel" class="col-sm-12 col-md-8 col-md-offset-2 carousel slide" data-ride="carousel">
                    <ol class="carousel-indicators">
                    {{#each carousel}}
                    {{#if @first}}
                        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    {{else}}
                        <li data-target="#myCarousel" data-slide-to="{{@index}}"></li>
                    {{/if}}
                    {{/each}}
                    </ol>
                    <div class="carousel-inner" role="listbox">
                    {{#each carousel}}
                    {{#if @first}}
                        <div class="item active">
                    {{else}}
                        <div class="item">
                    {{/if}}
                            <img src="{{path}}" alt="{{alt}}">
                        </div>
                    {{/each}}
                    </div>
                    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
            </div>

            <div class="row">
                <div class="col-sm-12 col-md-8 col-md-offset-2 project-description">
                    <hr class="line-full-2">
                    <div class="row">
                        <div class="col-sm-3">
                            <h3>About</h3>
                            <!-- <button onclick=\'document.getElementById("TL1").src="../resources/portfolio/TiledLoader/TiledLoader1.png";\' alt="TiledLoader 1">Set image</button> -->
                        </div>
                        <div class="col-sm-9">
                            {{{about}}}
                        </div>
                    </div>
                    <hr class="line-full-2">
                </div>
            </div>

            <div class="row project-write-up">
                <div class="col-sm-12 col-md-8 col-md-offset-2">
                    {{{write_up}}}
                </div>
            </div>
        </div>
    </section>

    {{> footer}}

    <!-- inject:js-->
    <script src="/dist/js/jquery.min.js"></script>
    <script src="/dist/js/bootstrap.min.js"></script>
    <script src="/dist/js/all.min.js"></script>
    <!-- endinject -->
</body>

</html>
